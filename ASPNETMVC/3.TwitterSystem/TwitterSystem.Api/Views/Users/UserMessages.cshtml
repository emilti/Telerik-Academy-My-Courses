@model List<TwitterSystem.Api.Models.Messages.MessageViewModel>

@foreach (var message in Model)
{
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">@message.Title<span class="pull-right">@message.CreatedOn</span></h3>
        </div>
        <div class="panel-body">
            @message.Content
        </div>
    </div>
}

